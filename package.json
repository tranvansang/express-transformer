{
  "name": "express-transformer",
  "version": "3.3.16",
  "description": "An express transformer, validator library",
  "main": "lib/transformer.js",
  "repository": "https://github.com/tranvansang/express-transformer",
  "author": "Tran Sang",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "private": false,
  "scripts": {
    "clean": "rm -rf lib",
