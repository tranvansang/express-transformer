{
  "name": "express-transformer",
  "version": "0.3.1",
  "description": "An express transformer, validator library",
  "main": "lib/transformer.js",
  "repository": "https://github.com/tranvansang/express-transformer",
  "author": "Tran Sang",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf lib && yarn babel src -d lib --extensions \".ts,.js\" && yarn build:types",
    "test": "yarn mocha --require @babel/generator --require @babel/polyfill --require babel-register-ts",
    "coverage": "yarn nyc mocha --require babel-register-ts --require @babel/polyfill --recursive",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "keywords": [
    "express",
    "connect",
    "validator",
    "transform",
    "transformer",
    "validation",
    "validate",
    "async",
    "await",
    "promise",
    "ES6",
    "chain",
    "multivalue",
    "reuse",
    "middleware"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/generator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-register-ts": "^7.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "middleware-async": "^0.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^6.1.5",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^3.2.0"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/validator": "^9.4.2",
    "validator": "^10.7.0"
  }
}
